{#
/**
 * @file usabilla_item.html.twig
 * Default theme implementation to present Usabilla item data.
 *
 * This template is used when viewing Usabilla item pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_usabilla_item()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('usabilla_item') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
